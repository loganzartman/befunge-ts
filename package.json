{
  "name": "befunge-ts",
  "version": "1.0.0",
  "description": "Befunge-93 interpreter for TypeScript",
  "files": [
    "dist/"
  ],
  "main": "dist/index.js",
  "scripts": {
    "test": "pnpm jest",
    "start": "pnpm parcel",
    "build": "pnpm build-lib && pnpm build-parcel",
    "build-parcel": "pnpm parcel build",
    "build-lib": "pnpm tsc -p src/lib",
    "clean": "rimraf dist dist-sandbox"
  },
  "packageManager": "pnpm@8.12.1",
  "keywords": [],
  "author": "Logan Zartman",
  "license": "MIT",
  "targets": {
    "sandbox": {
      "source": "src/sandbox/index.html",
      "distDir": "dist-sandbox"
    }
  },
  "alias": {
    "react": "preact/compat",
    "react-dom/test-utils": "preact/test-utils",
    "react-dom": "preact/compat",
    "react/jsx-runtime": "preact/jsx-runtime",
    "preact/jsx-dev-runtime": "preact/jsx-runtime"
  },
  "devDependencies": {
    "@codemirror/view": "^6.22.3",
    "@parcel/transformer-typescript-tsc": "^2.10.3",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "@uiw/codemirror-theme-vscode": "^4.21.21",
    "@uiw/react-codemirror": "^4.21.21",
    "codemirror": "^6.0.1",
    "dedent": "^1.5.1",
    "eslint": "^8.56.0",
    "eslint-config-preact": "^1.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-no-relative-import-paths": "^1.5.3",
    "eslint-plugin-prettier": "^5.1.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jest": "^29.7.0",
    "parcel": "^2.10.3",
    "postcss": "^8.4.32",
    "preact": "^10.19.3",
    "preact-dom": "^1.0.1",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
